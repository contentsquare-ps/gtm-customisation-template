___SANDBOXED_JS_FOR_WEB_TEMPLATE___

const copyFromWindow = require ('copyFromWindow');
const getUrl = require('getUrl');
const getQueryParameters = require('getQueryParameters');
const encodeUriComponent = require('encodeUriComponent');
const createQueue = require ('createQueue');
const makeInteger = require('makeInteger');
const makeNumber = require('makeNumber');
const getType = require('getType');

const logToConsole = require('logToConsole');

let _uxaPush = createQueue('_uxa');
let windowPath = getUrl('path');
let windowHash = getUrl('fragment');

switch(data.action_type){
  case 'artificial_pageview':
    if (windowHash !== '') {
      if(typeof data.artificial_pageview_query == 'undefined'){
        _uxaPush(['trackPageview', windowPath+'?__'+windowHash]);
      }
      else{
        _uxaPush(['trackPageview', windowPath]);
      }
    }
    else {
      if(typeof data.artificial_pageview_query == 'undefined'){
        _uxaPush(['trackPageview', windowPath]);
      }
      else{
        _uxaPush(['trackPageview', windowPath+'?cs_query='+data.artificial_pageview_query]);
      }
    }
    data.gtmOnSuccess();
    break;
  case 'dynamic_variable':
    var checkedValue;
    var dvarValue;
    for (var i = 0; i < data.dynamic_variables_table.length; i++) {
      //logToConsole('value type ' + data.dynamic_variables_table[i].value_type);
      dvarValue = data.dynamic_variables_table[i].dynamic_variable_value;
      if(data.dynamic_variables_table[i].value_type =='value_integer'){
        dvarValue = makeInteger(data.dynamic_variables_table[i].dynamic_variable_value);
      }
      _uxaPush(
        ["trackDynamicVariable", {
          key: data.dynamic_variables_table[i].dynamic_variable_key, 
          value: dvarValue
        }]
      );
      logToConsole('sent result ' + dvarValue + ' ' + (typeof dvarValue) );
    }
    data.gtmOnSuccess();
    break;
  case 'standard_page_event':
     _uxaPush(['trackPageEvent', data.page_event_value]);
    data.gtmOnSuccess();
    break;
  case 'ETP_page_event':
     _uxaPush(['trackEventTriggerRecording', '@ETP@'+data.etp_value]);
    data.gtmOnSuccess();
    break;
  case 'ETS_page_event':
     _uxaPush(['trackEventTriggerRecording', '@ETS@'+data.ets_value]);
     data.gtmOnSuccess();
    break;
  case 'add_to_cart':
     _uxaPush(['ec:cart:add', {sku: data.add_to_cart_sku}]);
    data.gtmOnSuccess();
    break;
}


